{
  "name": "vue-fun",
  "version": "1.0.0",
  "main": "index.js",
  "repository": {},
  "license": "MIT",
  "scripts": {
    "start-dev": "NODE_ENV=development concurrently \"nodemon --watch ./api ./server\" \"budo client/main.js:main.bundle.js --live --dir public -- --debug\"",
    "start-prod": "NODE_ENV=production concurrently \"browserify client/main.js -g uglifyify -o ./public/main.bundle.js\" \"node ./server\"",
    "test": "eslint --fix --ext .js,.vue ./client ./api ./tests && tape ./**/*.spec.js"
  },
  "browserify": {
    "transform": [
      "babelify",
      "vueify",
      "envify"
    ]
  },
  "dependencies": {
    "clone-deep": "^0.2.4",
    "corsify": "^2.1.0",
    "express": "^4.14.1",
    "is-my-json-valid": "^2.16.0",
    "level": "^1.6.0",
    "memdb": "^1.3.1",
    "request": "^2.79.0",
    "run-series": "^1.1.4",
    "vue": "^2.2.2",
    "vue-router": "^2.2.0",
    "vuex": "^2.1.2",
    "xhr": "^2.3.3"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-preset-es2015": "^6.22.0",
    "babelify": "^7.3.0",
    "browserify": "^14.0.0",
    "budo": "^9.4.7",
    "concurrently": "^3.3.0",
    "envify": "^4.0.0",
    "eslint": "^3.17.1",
    "eslint-config-standard": "^7.0.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^2.1.1",
    "eslint-plugin-vue": "^2.0.1",
    "hyperx": "^2.3.0",
    "json-query": "^2.2.0",
    "nodemon": "^1.11.0",
    "tape": "^4.6.3",
    "uglifyify": "^3.0.4",
    "vue-server-renderer": "^2.2.2",
    "vuegister": "^0.2.0",
    "vueify": "^9.4.0"
  }
}
